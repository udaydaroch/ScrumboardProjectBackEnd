{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://scrumboard-project-cbsb6q3lj-udaydarochs-projects.vercel.app/"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PATCH, PUT, DELETE"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept, X-Authorization"
        }
      ]
    }
  ]
}